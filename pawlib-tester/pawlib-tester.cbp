<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="pawlib-tester" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug64">
				<Option output="bin/Debug64/pawlib-tester" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug64/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-std=c++11" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-m64 -v" />
					<Add directory="../pawlib/Debug64/include" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="../pawlib/Debug64/include" />
				</ResourceCompiler>
				<Linker>
					<Add library="../pawlib/Debug64/lib/libpawlib.a" />
					<Add directory="../pawlib/Debug64/lib" />
				</Linker>
			</Target>
			<Target title="Release64">
				<Option output="bin/Release64/pawlib-tester" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release64/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-m64" />
					<Add directory="../pawlib/Release64/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="../pawlib/Release64/lib/libpawlib.a" />
					<Add directory="../pawlib/Release64/lib" />
				</Linker>
			</Target>
			<Target title="Debug32">
				<Option output="bin/Debug32/pawlib-tester" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug32/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-std=c++11" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-m32" />
					<Add directory="../pawlib/Debug32/include" />
				</Compiler>
				<Linker>
					<Add library="../pawlib/Debug32/lib/libpawlib.a" />
					<Add directory="../pawlib/Debug32/lib" />
				</Linker>
			</Target>
			<Target title="Release32">
				<Option output="bin/Release32/pawlib-tester" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release32/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-m32" />
					<Add directory="../pawlib/Release32/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="../pawlib/Release32/lib/libpawlib.a" />
					<Add directory="../pawlib/Release32/lib" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="Debug64;Release64;Debug32;Release32;" />
			<Add alias="Debug" targets="Debug64;Debug32;" />
			<Add alias="Release" targets="Release64;Release32;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add option="`pkg-config --cflags --libs sigc++-2.0`" />
		</Compiler>
		<Linker>
			<Add option="`pkg-config --libs sigc++-2.0`" />
		</Linker>
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1">
				<lib name="sigc++-2.0" />
			</lib_finder>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
